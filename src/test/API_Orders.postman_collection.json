	"item": [
		{
			"name": "Realizar una compra",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Extract values from response\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.environment.set(\"orderId\", responseJson.orderId);\r",
							"    pm.environment.set(\"success\", responseJson.success);\r",
							"    pm.environment.set(\"description\", responseJson.description);\r",
							"});\r",
							"// Test for the description attribute value in the response body\r",
							"pm.test(\"Description attribute is either 'no money.' or 'ok.'\", function () {\r",
							"    pm.expect(pm.response.json().description).to.be.oneOf(['no money.', 'ok.']);\r",
							"});// Test for a successful status code\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"// Test for the presence of orderId in the response body\r",
							"pm.test(\"orderId is present in the response\", function () {\r",
							"    pm.expect(pm.response.json()).to.have.property('orderId');\r",
							"});\r",
							"\r",
							"// Test for the type of success attribute in the response body\r",
							"pm.test(\"Type of success attribute is boolean\", function () {\r",
							"    pm.expect(pm.response.json().success).to.be.a('boolean');\r",
							"});\r",
							"\r",
							"// Test for the description attribute in the response body\r",
							"pm.test(\"Description attribute is not empty\", function () {\r",
							"    pm.expect(pm.response.json().description).to.not.be.empty;\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
